<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>com.hillel.hw19.ServletForSort</servlet-name>
    <servlet-class>com.hillel.hw19.ServletForSort</servlet-class>
  </servlet>

  <servlet>
    <servlet-name>com.hillel.hw19.ServletForFindText</servlet-name>
    <servlet-class>com.hillel.hw19.ServletForFindText</servlet-class>
  </servlet>


  <servlet-mapping>
    <servlet-name>com.hillel.hw19.ServletForSort</servlet-name>
    <url-pattern>/first</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>com.hillel.hw19.ServletForFindText</servlet-name>
    <url-pattern>/second</url-pattern>
  </servlet-mapping>
</web-app>
